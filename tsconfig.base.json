{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@fullwhere/commands/seed": ["libs/commands/seed/src/index.ts"],
      "@fullwhere/features/feedbacks/data-access": [
        "libs/features/feedbacks/data-access/src/index.ts"
      ],
      "@fullwhere/features/feedbacks/domain": [
        "libs/features/feedbacks/domain/src/index.ts"
      ],
      "@fullwhere/features/feedbacks/graphql": [
        "libs/features/feedbacks/graphql/src/index.ts"
      ],
      "@fullwhere/kernel/data-access/datasource": [
        "libs/kernel/data-access/datasource/src/index.ts"
      ],
      "@fullwhere/kernel/graphql": ["libs/kernel/graphql/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
